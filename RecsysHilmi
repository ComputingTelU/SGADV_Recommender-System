		print "bbb"
	else:
		print "ada yg salah"



matriks = [[0 for col in range(6)]for row in range(6)]

matriks[0][0] = 2
matriks[0][3] = 4
matriks[0][4] = 5
matriks[1][0] = 5
matriks[1][2] = 4
matriks[1][5] = 1
matriks[2][2] = 5
matriks[2][4] = 2
matriks[3][1] = 1
matriks[3][3] = 5
matriks[3][5] = 4
matriks[4][2] = 4
matriks[4][5] = 2
matriks[5][0] = 4
matriks[5][1] = 5
matriks[5][3] = 1

sigma("a")

