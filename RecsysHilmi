					elif (matriks[h][j] !=0) and (h ==3): 
						d = 1;
					h = h+1;
			j = j+1	


		if (a == 1) and (bhay == "a"):
			y = pembagi("a")
			i = 5
			j = 0
			h = 0
			nilai1 = 0
			nilai2 = 0
			nilai3 = 0
			nilai4 = 0
			while(j<6):
				if (matriks[i][j] != 0) and (matriks[h][j] != 0):
					if (nilai1 == 0) or (nilai2 == 0):
						nilai1 = matriks[i][j]
						nilai2 = matriks[h][j]
					else:
						nilai3 = matriks[i][j]
						nilai4 = matriks[h][j]
				j = j+1		
			if (nilai3 != 0):
				hasilatas = ((nilai1 - x)*(nilai2-y))+((nilai3 - x)*(nilai4-y))
				hasilbawah = math.sqrt(((nilai1-x)**2+(nilai3-x)**2)*((nilai2-y)**2+(nilai4-y)**2))
				hasil = hasilatas/hasilbawah
			elif (nilai3 == 0):
				hasilatas = (nilai1-x)*(nilai2-y)
				hasilbawah = math.sqrt((nilai1-x)**2*(nilai2-y)**2)
				hasil = hasilatas/hasilbawah
			return hasil	

