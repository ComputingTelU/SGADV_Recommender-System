def anggotanya(e):
	if e == "a":
		i = 0
		j = 0
		while j<6:
			print matriks[i][j]
			j = j+1
	elif e == "b":
		i = 1
		j = 0
		while j<6:
			print matriks[i][j]
			j = j+1
	elif e == "c":
		i = 2
		j = 0
		while j<6:
			print matriks[i][j]
			j = j+1
	elif e == "d":
		i = 3
		j = 0
		while j<6:
			print matriks[i][j]
			j = j+1
	elif e == "e":
		i = 4
		j = 0
		while j<6:
			print matriks[i][j]
			j = j+1
	elif e == "f":
		i = 5
		j = 0
		while j<6:
			print matriks[i][j]
			j = j+1
	else:
		print "Indeks nya gada bro"

def pembagi(e):
	if e == "a":
		i = 0
		j = 0
		a = 0.0
		b = 0.0
		while j<6:
			if matriks[i][j] != 0:
				a = a + matriks[i][j]
				b = b + 1
			j = j+1
		x = a/b
		return x
	elif e == "b":
		i = 1
		j = 0
		a = 0.0
		b = 0.0
		while j<6:
			if matriks[i][j] != 0:
				a = a + matriks[i][j]
				b = b + 1
			j = j+1
		x = a/b
		return x
	elif e == "c":
		i = 2
		j = 0
		a = 0.0
		b = 0.0
		while j<6:
			if matriks[i][j] != 0:
				a = a + matriks[i][j]
				b = b + 1
			j = j+1
		x = a/b
		return x
	elif e == "d":
		i = 3
		j = 0
		a = 0.0
		b = 0.0
		while j<6:
			if matriks[i][j] != 0:
				a = a + matriks[i][j]
				b = b + 1
			j = j+1
		x = a/b
		return x
	elif e == "e":
		i = 4
		j = 0
		a = 0.0
		b = 0.0
		while j<6:
			if matriks[i][j] != 0:
				a = a + matriks[i][j]
				b = b + 1
			j = j+1
		x = a/b
		return x
	elif e == "f":
		i = 5
		j = 0
		a = 0.0
		b = 0.0
		while j<6:
			if matriks[i][j] != 0:
				a = a + matriks[i][j]
				b = b + 1
			j = j+1
		x = a/b
		return x
	else:
		print "Indeks nya gada bro"

def similarity1(ahay,bhay):
	if ahay == "e":
		i = 4
		j = 0
		h = 0
		b = 0
		c = 0
		d = 0
		a = 0
		f = 0
		x = pembagi("e")
		while(j<6):
			if matriks[i][j] != 0:
				h = 0
				while(h<6):
					if (matriks[h][j] !=0) and (h ==1):
						b = 1;
					elif (matriks[h][j] !=0) and (h ==2): 
						c = 1;
					elif (matriks[h][j] !=0) and (h ==3): 
						d = 1;
					elif (matriks[h][j] !=0) and (h ==0): 
						a = 1;
					elif (matriks[h][j] !=0) and (h ==5): 
						f = 1;
					h = h+1;
			j = j+1	


		if (a == 1) and (bhay == "a"):
			y = pembagi("b")
			i = 4
			j = 0
			h = 0
			nilai1 = 0
			nilai2 = 0
			nilai3 = 0
			nilai4 = 0
			while(j<6):
				if (matriks[i][j] != 0) and (matriks[h][j] != 0):
					if (nilai1 == 0) or (nilai2 == 0):
						nilai1 = matriks[i][j]
						nilai2 = matriks[h][j]
					else:
						nilai3 = matriks[i][j]
						nilai4 = matriks[h][j]
				j = j+1		
			if (nilai3 != 0):
				hasilatas = ((nilai1 - x)*(nilai2-y))+((nilai3 - x)*(nilai4-y))
				hasilbawah = math.sqrt(((nilai1-x)**2+(nilai3-x)**2)*((nilai2-y)**2+(nilai4-y)**2))
				hasil = hasilatas/hasilbawah
			elif (nilai3 == 0):
				hasilatas = (nilai1-x)*(nilai2-y)
				hasilbawah = math.sqrt((nilai1-x)**2*(nilai2-y)**2)
				hasil = hasilatas/hasilbawah
			return hasil	

		elif (b == 1) and (bhay == "b"):
			y = pembagi("a")
			i = 4
			j = 0
			h = 1
			nilai1 = 0
			nilai2 = 0
