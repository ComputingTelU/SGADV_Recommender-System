import math

def tetangganya(e):
	if e == "a":
		i = 0
		j = 0
		h = 1
		b = 0
		c = 0
		d = 0
		e = 0
		f = 0
		while(j<6):
			if matriks[i][j] != 0:
				h = 1
				while(h<6):
					if (matriks[h][j] !=0) and (h ==1):
						b = 1;
					elif (matriks[h][j] !=0) and (h ==2): 
						c = 1;
					elif (matriks[h][j] !=0) and (h ==3): 
						d = 1;
					elif (matriks[h][j] !=0) and (h ==4): 
						e = 1;
					elif (matriks[h][j] !=0) and (h ==5): 
						f = 1;
					h = h+1;
			j = j+1	
		if (b == 1):
			bnya = "b"
		else:
			bnya = ""

